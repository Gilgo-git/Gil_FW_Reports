        <CommandText>------------------------------------------------------
-- D38 - Liste des AWD en double à vérifier (Investia)
------------------------------------------------------
if OBJECT_ID('tempdb..#AWD') is not null
        drop table #AWD
  

--SELECT 'AWD en double'
SELECT OTX.IVR_SYSID, OTX.PLN_SYSID, OTX.ACT_SYSID, TRX_GROSS, COUNT(*) AS NBR
INTO #AWD
FROM OTX (nolock), S_TRX_CD (nolock), PLN (nolock)
WHERE OTX.TRX_CD = S_TRX_CD.TRX_CD
	AND TRX_MNEM_ENG = 'AWD'		
	AND OTX.TRADE_DT = @DATE  -- CONVERT(VARCHAR(10),GETDATE(),121)														
	AND OTX.PLN_SYSID = PLN.PLN_SYSID
	AND PLN_ADM_CD = 'INV'					
GROUP BY OTX.IVR_SYSID, OTX.PLN_SYSID, OTX.ACT_SYSID, TRX_GROSS
ORDER BY 5 DESC

SELECT * FROM #AWD WHERE NBR &gt; 1</CommandText>
